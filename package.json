{
  "name": "simple-widgets",
  "version": "1.30.8",
  "description": "A Collection of React components to make your React code simpler",
  "main": "./index.js",
  "scripts": {
    "update": "npm install --legacy-peer-deps --silent ; npx npm-force-resolutions ; npm audit",
    "setup": "npm install  --legacy-peer-deps --silent",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "build": "npm run buildIndex && webpack",
    "dry": "run-s build dry-run",
    "pub": "run-s build release",
    "test": "jest --colors",
    "watch": "jest --watch",
    "coverage": "jest --coverage && cat ./tests/coverage/lcov.info ",
    "example": "See https://github.com/martinjackson/simple-widgets-sample.git",
    "dry-run": "release-it --dry-run -n --no-git.requireCleanWorkingDir",
    "release": "release-it --no-git.requireCleanWorkingDir",
    "buildIndex": "cd src && node buildIndex.js"
  },
  "resolutions": {
    "got": "11.8.5",
    "terser": "5.14.2",
    "parse-path": "6.0.5",
    "parse-url": "8.1.0",
    "minimist": "1.2.6",
    "minimatch": "3.0.5",
    "glob-parent": "6.0.2",
    "yargs-parser": "21.0.1",
    "braces": "3.0.2",
    "concat-stream": "2.0.0",
    "lodash.template": "4.5.0",
    "vm2": "3.9.11",
    "ajv": "6.12.6"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-flow"
    ]
  },
  "jest": {
    "browser": true,
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/obs/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!test/**/*.test.js",
      "!test/webpack.config.js",
      "!test/ExampleApp.js",
      "!src/PrevDoubleListBox/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjackson/simple-widgets.git"
  },
  "keywords": [
    "React",
    "checkbox",
    "choice",
    "pulldown",
    "pdf",
    "radiobutton"
  ],
  "author": "Martin A. Jackson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martinjackson/simple-widgets/issues"
  },
  "homepage": "https://github.com/martinjackson/simple-widgets#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-flow": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": ">=18.0.11",
    "acorn-dynamic-import": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "9.1.2",
    "core-js": "^3.30.0",
    "css-loader": "^6.7.3",
    "esbuild": "^0.17.15",
    "esbuild-plugin-inline-image": "^0.0.9",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "flow-bin": "^0.203.1",
    "jest": "^29.5.0",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "release-it": "^15.10.1",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "strip-comments": "^2.0.1",
    "style-loader": "^3.3.2",
    "wallaby-webpack": "^3.9.16",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "peerDependencies": {
    "@types/react": ">=17.0.27",
    "autoprefixer": ">=10.4.14",
    "pdfmake": ">=0.2.7",
    "react": ">=17.0.2",
    "react-csv": ">=2.2.2",
    "react-dom": ">=17.0.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "babel-node": "^0.0.1-security",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "react-day-picker": "^8.6.0",
    "react-html-comment": "^2.0.16",
    "react-json-view": "^1.21.3",
    "react-spinners": "^0.13.8",
    "zustand": "^4.3.7"
  }
}
