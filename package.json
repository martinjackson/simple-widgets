{
  "name": "simple-widgets",
  "version": "1.33.10",
  "description": "A Collection of React components to make your React code simpler",
  "main": "./lib/index.js",
  "scripts": {
    "dont-use-build2": "rm -f lib/index.*js* && node build.js && ls -l lib/ ",
    "update": "npm install --legacy-peer-deps --silent; npx npm-force-resolutions ; npm audit",
    "setup": "npm install  --legacy-peer-deps",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "build": "./build-prep.sh && webpack",
    "dry": "run-s build dry-run",
    "pub": "run-s build release",
    "test": "jest --colors",
    "watch": "jest --watch",
    "coverage": "jest --coverage && cat ./tests/coverage/lcov.info ",
    "example": "See https://github.com/martinjackson/simple-widgets-sample.git",
    "dry-run": "release-it --dry-run -n --no-git.requireCleanWorkingDir",
    "release": "release-it --no-git.requireCleanWorkingDir"
  },
  "resolutions": {
    "got": "11.8.5",
    "terser": "5.14.2",
    "parse-path": "6.0.5",
    "parse-url": "8.1.0",
    "minimist": "1.2.6",
    "minimatch": "3.0.5",
    "glob-parent": "6.0.2",
    "yargs-parser": "21.0.1",
    "braces": "3.0.2",
    "concat-stream": "2.0.0",
    "lodash.template": "4.5.0",
    "vm2": ">=3.9.17"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-flow"
    ]
  },
  "jest": {
    "browser": true,
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/obs/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!test/**/*.test.js",
      "!test/webpack.config.js",
      "!test/ExampleApp.js",
      "!src/PrevDoubleListBox/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjackson/simple-widgets.git"
  },
  "keywords": [
    "React",
    "checkbox",
    "choice",
    "pulldown",
    "pdf",
    "radiobutton"
  ],
  "author": "Martin A. Jackson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martinjackson/simple-widgets/issues"
  },
  "homepage": "https://github.com/martinjackson/simple-widgets#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/node": "^7.20.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "acorn-dynamic-import": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.2",
    "babel-loader": "^9.0.0",
    "core-js": "^3.26.0",
    "css-loader": "^6.7.1",
    "esbuild": "^0.17.15",
    "esbuild-plugin-inline-image": "^0.0.9",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "flow-bin": "^0.191.0",
    "jest": "^29.2.2",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.10",
    "release-it": "^15.5.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "strip-comments": "^2.0.1",
    "style-loader": "^3.3.1",
    "vm2": ">=3.9.17",
    "wallaby-webpack": "^3.9.16",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.7.11",
    "axios": "^1.3.5",
    "deep-object-diff": "^1.1.9",
    "graphql-tag": "^2.12.6",
    "object-sizeof": "^2.6.1",
    "@types/react": "^17.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "autoprefixer": "^10.4.14",
    "babel-node": "^0.0.1-security",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "pdfmake": "^0.2.7",
    "react-autobind": "^1.0.6",
    "react-calendar": "^3.9.0",
    "react-csv": "^2.2.2",
    "react-day-picker": "^8.3.5",
    "react-html-comment": "^2.0.14",
    "react-json-view": "^1.21.3",
    "react-spinners": "^0.13.6",
    "zustand": "^4.1.3"
  }
}
