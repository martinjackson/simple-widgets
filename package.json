{
  "name": "simple-widgets",
  "version": "1.41.9",
  "description": "A Collection of React components to make your React code simpler",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dont-use-build2": "rm -f lib/index.*js* && node build.js && ls -l lib/ ",
    "update": "npm install --legacy-peer-deps --silent; npx npm-force-resolutions ; npm audit",
    "setup": "npm install  --legacy-peer-deps",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "build": "./build-prep.sh && webpack && npm pack --silent ",
    "dry": "run-s build dry-run",
    "pub": "run-s build release",
    "test-lib": "echo '// Install the local package \n   npm install '../location/of/your/local/project' ",
    "test": "jest --colors",
    "watch": "jest --watch",
    "coverage": "jest --coverage && cat ./tests/coverage/lcov.info ",
    "example": "See https://github.com/martinjackson/simple-widgets-sample.git",
    "dry-run": "release-it --dry-run -n --no-git.requireCleanWorkingDir",
    "release": "release-it --no-git.requireCleanWorkingDir"
  },
  "resolutions": {
    "got": "11.8.5",
    "terser": "5.14.2",
    "parse-path": "6.0.5",
    "parse-url": "8.1.0",
    "minimist": "1.2.6",
    "minimatch": "3.0.5",
    "glob-parent": "6.0.2",
    "yargs-parser": "21.0.1",
    "concat-stream": "2.0.0",
    "lodash.template": "4.5.0",
    "vm2": ">=3.9.17",
    "optionator": "0.9.3",
    "semver": "7.5.4",
    "postcss": "8.4.31",
    "word-wrap": "1.2.5",
    "braces": "3.0.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ],
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/preset-flow"
    ]
  },
  "jest": {
    "browser": true,
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.{js,jsx}",
      "!**/src/obs/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!test/**/*.test.js",
      "!test/webpack.config.js",
      "!test/ExampleApp.js",
      "!src/PrevDoubleListBox/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinjackson/simple-widgets.git"
  },
  "keywords": [
    "React",
    "checkbox",
    "choice",
    "pulldown",
    "pdf",
    "radiobutton"
  ],
  "author": "Martin A. Jackson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martinjackson/simple-widgets/issues"
  },
  "homepage": "https://github.com/martinjackson/simple-widgets#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-flow": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "acorn-dynamic-import": "^4.0.0",
    "ajv": "^8.12.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-node": "^0.0.1-security",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "esbuild": "^0.19.5",
    "esbuild-plugin-inline-image": "^0.0.9",
    "eslint": "^8.51.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "flow-bin": "^0.219.2",
    "jest": "^29.7.0",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-test-renderer": "^17.0.0",
    "regenerator-runtime": "^0.14.0",
    "release-it": "^16.2.1",
    "sass": "^1.69.4",
    "sass-loader": "^13.3.2",
    "strip-comments": "^2.0.1",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.7.11",
    "@types/react": "17 - 18",
    "axios": "^1.3.5",
    "deep-object-diff": "^1.1.9",
    "graphql-tag": "^2.12.6",
    "object-sizeof": "^2.6.1",
    "react": "17 - 18",
    "react-dom": "17 - 18"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "pdfmake": "^0.2.7",
    "react-autobind": "^1.0.6",
    "react-calendar": "^4.6.1",
    "react-csv": "^2.2.2",
    "react-spinners": "^0.13.8",
    "zustand": "^4.4.3"
  }
}
